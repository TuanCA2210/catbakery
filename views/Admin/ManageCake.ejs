  <section class="content">
    <div class="row">
      <div class="col-xs-12">
        <div class="box">
          <div class="box-header">
            <h3 class="box-title">Cakes</h3>
          </div>
          <!-- /.box-header -->
          <div class="box-body">
            <form action="/AdminInterface/PrintCakeSearch" method="GET">
              <div class="input-group" style="width: 40%;">
                <input type="text" class="form-control" name="cake" placeholder="Search">
                <span class="input-group-btn">
                  <button  class="btn btn-default" type="submit" value="search" style="margin-top: 0px;"><span class="glyphicon glyphicon-search"></span></button>
                </span>
              </div>
            </br>
            <div>
            <select name="catyegory">
            <option value="" default>Choose role you want</option>
            <%if(resultCakeCategory.length>0){%>
            <%for(var i =0 ; i < resultCakeCategory.length; i++){%>
            <option value="<%=resultCakeCategory[i]._id%>"><%=resultCakeCategory[i].cake_type%></option>
            <%}}%>

            </select>
            </div>
            </br>
            </form>
            <br/>
            <table id="ManageCake" class="table table-bordered table-hover">
              <thead>
              <tr>
                <th>Cake Name</th>
                <th>Description</th>
                <th>Material</th>
                <th>Size</th>
                <th>Price</th>
                <th>Quantity</th>
                <th>Status</th>
                <th style="width: 70px;"></th>
                <th style="width: 70px;"></th>
                <th style="width: 70px;"></th>
              </tr>
            </thead>
            <tbody>
              <%if (result.length > 0) {%>
                <% for(var i=0;i<result.length; i++){%>


                  <tr>

                    <td><%= result[i].name_cake%></td>
                    <td><%= result[i].description%></td>
                    <td>

                      <% for(var n=0;n<result[i].material.length; n++){%>
                        <option value="<%= result[i].material[n]%>"  selected="selected"><%= result[i].material[n]%></option>
                        <%}%>

                      </td>
                      <td><%= result[i].name_size%></td>
                      <td id="price"><%= result[i].price%></td>
                      <td><%= result[i].quantity%></td>
                      <%if(result[i].status == false){ %>
                        <td>Selling</td>
                        <%}else{%>
                          <td>Not Sell</td>
                          <%}%>
                          <td><button type="button" class="btn btnLarge btn-warning" onclick="window.location.href='/AdminInterface/CakeSell?id=<%=result[i]._id%>';">Sell Again</button></td>
                          <td><button type="button" class="btn btnLarge btn-info" onclick="window.location.href='/AdminInterface/NotCakeSell?id=<%=result[i]._id%>';">Not Sell</button></td>
                          <td>
                            <button type="button" class="btn btnLarge btn-primary" name="Update" value ="Update" onclick="window.location.href='/AdminInterface/InfoUpdateCake?id=<%= result[i]._id%>';">Update</button>
                          </td>
                        </tr>
                        <% }} %>
                      </tbody>
                      <tfoot>
                      </tfoot>
                      </table>
                      <!-- /.box-body -->
                    </div>
                  </div>
                </div>
                  <!-- /.col -->
                </div>
                <!-- /.row -->
              </section>
              <!-- /.content -->
            </div>

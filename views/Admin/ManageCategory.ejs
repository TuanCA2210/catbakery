        <div class="box">
          <div class="box-header">
            <h3 class="box-title">Category</h3>
          </div>
          <!-- /.box-header -->
          <div class="box-body">

            <table id="ManageCategory" class="table table-bordered table-hover">
              <tr>
                <th>Category Name</th>
              </tr>


              <%if (result.length > 0) {%>
                <% for(var i=0;i<result.length; i++){%>
                  <tr>
                    <td >
                      <div class="panel-heading">
                        <h3 class="panel-title" ><%=result[i].cake_type%></h3>

                        <%if(result[i].blocked == false){%>
                          <h3 class="panel-title" align="right"> Status : <strong>UnLock</strong></h3>
                          <%}else{%>
                            <h3 class="panel-title" align="right"> Status : <strong>Lock</strong></h3>
                            <%}%>


                          </div>
                        </td>


                        <div class="col-md-2">
                          <td style="width: 70px;"><button type="button" class="btn btnLarge btn-warning" onclick="window.location.href='/AdminInterface/LockCategory?id=<%=result[i]._id%>';">Lock</button></td>
                          <td style="width: 70px;"><button type="button" class="btn btnLarge  btn-info" onclick="window.location.href='/AdminInterface/UnLockCategory?id=<%=result[i]._id%>';">UnLock</button></td>
                        </div></tr>
                        <% }} %>

                      </table>
                      <!-- /.box-body -->
                    </div>
                  </div>
                  <!-- /.col -->
                </div>

            <div class="box-header">
              <h3 class="box-title">Update Account</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
							<form action="/AdminInterface/UpdateStaff" method ="post" enctype="multipart/form-data">
						    <div class="col-md-3 ab_pic_w3ls_text_info">
<span style="color:red" ><h3><strong><%=message%></strong></h3></span>
								<input  type="hidden" class="form-control"  name= "staff[id]" value="<%=result._id%>" ></br>
						    <p>Staff Name <span style="color: red">*</span></p><input type="text" class="form-control" placeholder="Staff Name" name="staff[name]" value="<%=result.name%>" required> </br>
						    <p>Password <span style="color: red">*</span></p><input type="text" class="form-control" placeholder="Password" name="staff[password]" value="<%=result.password%>" required> </br>
						    <p>Address <span style="color: red">*</span></p><input type="text" class="form-control" placeholder="Address" name ="staff[address]" value="<%=result.address%>" required>		</br>
						    <p>Phone Number <span style="color: red">*</span></p><input type="phone" class="form-control" placeholder="Phone Number" name ="staff[phone]" value="<%=result.phone%>" required></br>
						    <p>Email <span style="color: red">*</span></p><input type="email" class="form-control" placeholder="Email" name= "staff[email]" value="<%=result.email%>" required></br>
						    <p>Note </p><input type="text" class="form-control" placeholder="Note" name= "staff[note]" value="<%=result.note%>" ></br>


                <button type="button" class="btn btn-danger" onclick="window.location.href='/AdminInterface/HumanManage';">Cancel</button>
						    <button type="submit" class="btn btn-primary" value="Submit" >Update</button>
						  </div>
              <div class="col-md-6 ab_pic_w3ls">
                <br>
                <p>Image <span style="color: red">*</span></p>
                <img  src="/<%=result.linkImage%>" alt width="100" height="100"></br>
                <input type ="file" name ="file" ></br>
              </div>
							</form>

            </div>
            <!-- /.box-body -->
